{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "env": ["NODE_ENV", "PROD"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "eslint": {
      "dependsOn": [],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
